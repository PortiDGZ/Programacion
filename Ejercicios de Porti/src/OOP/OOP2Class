package OOP;

public class Contador{

    //Atributos

    int numero;


    //Constructores

    public Contador(int numero){

        this.numero = numero;

    }

    public Contador(){

      numero = 0;
    }

    public Contador(Contador permitecopia){

        numero = permitecopia.numero;

    }


    //Setters

    public void setNumero(int numero) {
        this.numero = numero;
    }

    //Getters

    public int getNumero() {
        return numero;
    }

    //MÃ©todos


}